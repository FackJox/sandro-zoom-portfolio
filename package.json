{
	"name": "svelte-scrollytelling",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "panda cssgen && concurrently \"panda --watch\" \"vite dev\" --names panda,vite -c yellow,blue",
		"build": "panda codegen && panda cssgen && vite build",
		"preview": "vite preview",
		"prepare": "panda codegen && svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@pandacss/dev": "^1.7.2",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"concurrently": "^9.2.1",
		"postcss": "^8.5.6",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"vite": "^7.2.6"
	},
	"dependencies": {
		"gsap": "^3.14.2"
	}
}
